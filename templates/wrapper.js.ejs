(function () {
    var <%= namespace %> = {
        modules: {},
        req: function (cwd) {
            cwd = cwd.replace(/\/$/, '');
            return function (modName) {
                var p = [cwd, modName].join('/');
                return <%= namespace %>.modules[p];
            };
        },
    };

<% mods.split(/\n/).forEach(function (line) { %>
    <%- line %><% }); %>
})();
