    var _embedRequire = {
        modules: {},
        req: function (cwd) {
            cwd = cwd.replace(/\/$/, '');
            return function (modName) {
                var p = [cwd, modName].join('/');
                return _embedRequire.modules[p];
            };
        },
    };

